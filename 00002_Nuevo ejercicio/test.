describe ("Prueba de función") 
{
  it ("Ingrese su nombre\n")
  {
    prueba();
    should_string(salidas[0]) be equal to("Ingrese su nombre\n");
  } end
  
  it ("Ingrese su edad\n")
  {
    prueba();
    should_string(salidas[1]) be equal to("Ingrese su edad\n");
  } end
  
  it ("Su nombre es Juan y tiene 35 años")
  {
    should_string(salidas[0]) be equal to("Su nombre es Juan y tiene 35 años");
  } end
  
} end